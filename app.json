{
  "expo": {
    "name": "MyTrackker",
    "slug": "MyTrackker",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "entryPoint": "./src/App.js",
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,

      "bundleIdentifier": "com.kimmosoft.mytrackker"
    },
    "android": {
      "package": "com.kimmosoft.mytrackker",
      "config": {
        "googleMaps": { "apiKey": "supersecretapikey" }
     }
    }
  }
}